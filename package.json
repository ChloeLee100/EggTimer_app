{
  "name": "egg-timer",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.eggtimer",
    "productName": "Egg Timer",
    "icon": "images/icon.icns",
